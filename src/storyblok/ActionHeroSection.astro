---
import type { ActionHeroSectionBlok } from "../types/storyblok/test-space";

type Props = {
  blok: ActionHeroSectionBlok;
};

const { blok } = Astro.props;
---

<a
  href={`/${blok.href?.cached_url}`}
  class="rounded-full border-2 border-white hover:bg-white hover:text-black transition-colors duration-500 px-4 py-2"
>
  {blok.label}
</a>
